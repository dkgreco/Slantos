                        <% include ./charts/bar %>
                    </div>
                    <div id="lineChart" class="tab-pane fade active">
                        <h3 class="submenuHeader">Line Chart</h3>
                        <% include ./charts/line %>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
